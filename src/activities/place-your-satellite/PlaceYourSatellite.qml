            id:buttonLaunch
            anchors.left: parent.left
            anchors.leftMargin: 0.3 * parent.width
            anchors.bottom: parent.bottom
            anchors.bottomMargin: parent.height * 0.2
            width: parent.width * 0.1
            visible:false
            onClicked: Activity.calcparameters()
